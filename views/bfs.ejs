    <script>
        function audio() {
            var audio = document.getElementById('music')
            var acon = document.getElementById('audiocon')
            if (audio.paused) {
                console.log('paused');
                audio.play();
                acon.className = "fa fa-volume-up";
            } else {
                console.log('playing');
                audio.pause();
                acon.className = "fa fa-volume-off";
            }
        }
        $(document).ready(function() {
            var audio = document.getElementById("music");
            audio.volume = 0.12;

        });
    </script>
    <style>
        #notfound {
